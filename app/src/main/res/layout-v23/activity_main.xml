<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.asus_pc.testing.MainActivity">

    <ImageView
        android:id="@+id/LawakKampus"
        android:layout_width="150dp"
        android:layout_height="159dp"
        android:layout_marginBottom="50dp"
        android:layout_marginLeft="47dp"
        android:contentDescription="@string/gambarlawakkampus"
        android:contextClickable="false"
        app:layout_constraintBottom_toTopOf="@+id/LawakKampus"
        app:layout_constraintLeft_toRightOf="@+id/LawakKampus"
        app:srcCompat="@mipmap/lawakkampus"
        tools:ignore="RtlHardcoded"
        android:layout_marginStart="47dp" />

    <ImageView
        android:id="@+id/Ujang"
        android:layout_width="150dp"
        android:layout_height="159dp"
        android:layout_marginLeft="200dp"
        android:layout_marginTop="50dp"
        android:contentDescription="@string/gambarujang"
        app:layout_constraintLeft_toRightOf="@+id/Ujang"
        app:layout_constraintTop_toBottomOf="@+id/Ujang"
        app:srcCompat="@mipmap/ujang"
        tools:ignore="RtlHardcoded"
        android:layout_marginStart="200dp" />

    <ImageView
        android:id="@+id/Under18"
        android:layout_width="150dp"
        android:layout_height="159dp"
        android:layout_marginLeft="-158dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="250dp"
        android:contentDescription="@string/app_name"
        app:layout_constraintLeft_toRightOf="@+id/Under18"
        app:layout_constraintTop_toBottomOf="@+id/Under18"
        app:srcCompat="@mipmap/under18"
        tools:ignore="RtlHardcoded" />



    <Button
        android:id="@+id/lawakkampus"
        android:layout_width="140dp"
        android:layout_height="48dp"
        android:layout_marginRight="50dp"
        android:layout_marginTop="200dp"
        android:contextClickable="true"
        android:text="@string/lawak_kampus"
        app:layout_constraintRight_toLeftOf="@+id/lawakkampus"
        app:layout_constraintTop_toBottomOf="@+id/lawakkampus"
        tools:ignore="RtlHardcoded"
        android:layout_marginEnd="50dp"
        android:onClick="SecondPage"
        />

    <Button
        android:id="@+id/ujang"
        android:layout_width="140dp"
        android:layout_height="48dp"
        android:layout_marginLeft="207dp"
        android:layout_marginStart="207dp"
        android:layout_marginTop="200dp"
        android:contextClickable="true"
        android:text="@string/ujangg"
        app:layout_constraintLeft_toRightOf="@+id/Ujang"
        app:layout_constraintTop_toBottomOf="@+id/Ujang"
        tools:ignore="RtlHardcoded" />

    <Button
        android:id="@+id/underlapanbelas"
        android:layout_width="140dp"
        android:layout_height="48dp"
        android:layout_marginLeft="133dp"
        android:layout_marginTop="403dp"
        android:contextClickable="true"
        android:elevation="0dp"
        android:text="@string/button"
        app:layout_constraintLeft_toRightOf="@+id/underlapanbelas"
        app:layout_constraintTop_toBottomOf="@+id/underlapanbelas"
        tools:ignore="RtlHardcoded" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="381dp"
        android:layout_height="38dp"
        android:text="@string/textview"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Button"
        android:textSize="24sp"
        app:layout_constraintRight_toLeftOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:ignore="RtlHardcoded"
        android:layout_marginTop="-43dp"
        android:layout_marginRight="-199dp" />




</android.support.constraint.ConstraintLayout>
