<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.asus_pc.testing.MainActivity">

    <ImageView
        android:id="@+id/LawakKampus"
        android:layout_width="137dp"
        android:layout_height="166dp"
        android:layout_margin="50dp"
        android:layout_marginTop="0dp"
        android:contentDescription="@string/gambarlawakkampus"
        android:contextClickable="false"
        app:layout_constraintBottom_toTopOf="@+id/LawakKampus"
        app:layout_constraintLeft_toRightOf="@+id/LawakKampus"
        app:srcCompat="@mipmap/lawakkampus"
        tools:ignore="RtlHardcoded" />

    <ImageView
        android:id="@+id/Ujang"
        android:layout_width="150dp"
        android:layout_height="159dp"
        android:layout_marginLeft="200dp"
        android:layout_marginTop="50dp"
        android:contentDescription="@string/gambarujang"
        app:layout_constraintLeft_toRightOf="@id/Ujang"
        app:layout_constraintTop_toBottomOf="@id/Ujang"
        app:srcCompat="@mipmap/ujang"
        tools:ignore="RtlHardcoded" />


    <Button
        android:id="@+id/lawakkampus"
        android:layout_width="140dp"
        android:layout_height="45dp"
        android:layout_marginRight="50dp"
        android:layout_marginTop="200dp"
        android:contextClickable="true"
        android:text="@string/lawak_kampus"
        app:layout_constraintRight_toLeftOf="@+id/lawakkampus"
        app:layout_constraintTop_toBottomOf="@+id/lawakkampus"
        tools:ignore="RtlHardcoded" />

    <Button
        android:id="@+id/ujang"
        android:layout_width="140dp"
        android:layout_height="45dp"
        android:layout_marginLeft="200dp"
        android:layout_marginTop="200dp"
        android:contextClickable="true"
        android:text="@string/ujangg"
        app:layout_constraintLeft_toRightOf="@id/ujang"
        app:layout_constraintTop_toBottomOf="@id/ujang"
        tools:ignore="RtlHardcoded" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="381dp"
        android:layout_height="38dp"
        android:layout_marginRight="-420dp"
        android:layout_marginTop="-25dp"
        android:text="@string/textview"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Button"
        android:textSize="24sp"
        app:layout_constraintRight_toLeftOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:ignore="RtlHardcoded" />





</android.support.constraint.ConstraintLayout>
